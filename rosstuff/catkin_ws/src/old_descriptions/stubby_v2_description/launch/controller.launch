<launch>

<rosparam file="$(find stubby_v2_description)/launch/controller.yaml" command="load"/>
<node args="Rev1_position_controller Rev2_position_controller Rev3_position_controller platformAngle_position_controller joint_state_controller " name="controller_spawner" ns="stubby_v2" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/stubby_v2/joint_states"/>
</node>

</launch>